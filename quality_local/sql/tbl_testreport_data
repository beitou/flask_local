CREATE TABLE `tbl_testreport_data` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `t_os_name` varchar(100) DEFAULT NULL,
  `t_browsers` varchar(100) DEFAULT NULL,
  `t_phones` varchar(100) DEFAULT NULL,
  `t_link` varchar(100) DEFAULT NULL,
  `t_methods` varchar(1024) DEFAULT NULL,
  `t_summary` varchar(1024) DEFAULT NULL,
  `t_memo` varchar(1024) DEFAULT NULL,
  `bug_fatal` varchar(100) DEFAULT NULL,
  `bug_serious` varchar(100) DEFAULT NULL,
  `bug_general` varchar(100) DEFAULT NULL,
  `bug_low` varchar(100) DEFAULT NULL,
  `bug_suggest` varchar(1024) DEFAULT NULL,
  `bug_new` varchar(1024) DEFAULT NULL,
  `bug_processing` varchar(1024) DEFAULT NULL,
  `bug_resolve` varchar(100) DEFAULT NULL,
  `bug_refuse` varchar(1024) DEFAULT NULL,
  `bug_closed` varchar(1024) DEFAULT NULL,
  `bug_function` varchar(1024) DEFAULT NULL,
  `bug_data` varchar(1024) DEFAULT NULL,
  `bug_ui` varchar(1024) DEFAULT NULL,
  `bug_compatibility` varchar(100) DEFAULT NULL,
  `bug_performance` varchar(1024) DEFAULT NULL,
  `bug_security` varchar(1024) DEFAULT NULL,
  `t_create_at` datetime DEFAULT NULL,
  `submit_data_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `submit_data_id` (`submit_data_id`),
  CONSTRAINT `tbl_testreport_data_ibfk_1` FOREIGN KEY (`submit_data_id`) REFERENCES `tbl_submit_data` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;
